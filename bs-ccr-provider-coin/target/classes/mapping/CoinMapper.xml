<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.bosenkeji.mapper.CoinMapper">
    <select id="findById" resultType="cn.bosenkeji.vo.Coin" parameterType="int">
        select id,name,status,created_at,updated_at from coin WHERE id=#{id} ;
    </select>
    <select id="findAll" resultType="cn.bosenkeji.vo.Coin">
        SELECT id,name,status from coin;
    </select>
    <insert id="create" parameterType="cn.bosenkeji.vo.Coin">
        INSERT INTO coin(name) VALUES (#{name}) ;
    </insert>
</mapper>